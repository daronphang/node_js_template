{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "plugins": [
        "prettier"
      ],
      "rules": {
        "prettier/prettier": ["error", { "singleQuote": true, "trailingComma": "all" }],
        "require-jsdoc" : 0,
        "new-cap": 0,
        "no-unused-vars": "off",
        "no-console": "off",
        "no-underscore-dangle": "off",
        "func-names": "off",
        "object-curly-newline": "off",
        "no-param-reassign": "off",
      },
    "parserOptions": {
        "ecmaVersion": 12
    }
}
